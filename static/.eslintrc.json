{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "plugins": ["plugin"],
  "settings": {
    "import/resolver": {
      "node": true
    }
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/triple-slash-reference": "off"
  },
  "globals": {
    "$": true
  }
}
